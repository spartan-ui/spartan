{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "react-app"
  ],
  "rules": {
    "@typescript-eslint/explicit-function-return-type": 0,
    "import/no-default-export": 2
  },
  "ignorePatterns": ["*.d.ts"]
}
